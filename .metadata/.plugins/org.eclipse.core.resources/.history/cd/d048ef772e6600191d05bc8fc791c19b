package com.kh.day2;

public class Run {

	public static void main(String[] args) {
		// 변수선언
		// 메소드내부(중괄호 안), 클래스내부(중괄호 안)
		//논리형 선언
		//이름 : boolean
		boolean flag;
		
		//단일문자
		//이름 : char
		char gender;
		
		//문자열
		//이름 : String
		String name;
		
		//숫자형(정수,실수)
		//정수
		//byte, short, int, long
		byte data;
		short num;
		int age;
		long account;
		
		
		
	}
}
