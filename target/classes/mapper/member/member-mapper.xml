<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!-- membermapper -->
<mapper namespace="member">
	<select id="selectMemberOne" resultType="member" parameterType="member">
		SELECT * FROM MEMBER WHERE ID = ${member.userId} AND PASSWORD = ${member.password} 
	</select>
</mapper>
